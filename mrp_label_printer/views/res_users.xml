<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!-- Adding default label printer in user form -->
        <record model="ir.ui.view" id="label_printer_res_users">
		    <field name="name">label.printer.res.user.form</field>
		    <field name="model">res.users</field>
		    <field name="inherit_id" ref="base.view_users_form" />
		    <field name="arch" type="xml">
			    <notebook position="inside">
				    <page string="Manufacture">
                        <group>
                            <field name="label_printer_id"/>
                        </group>
				    </page>
			    </notebook>
		    </field>
	    </record>
    </data>
</openerp>
